{
  "name": "react-pwa",
  "version": "1.0.0",
  "scripts": {
    "clean": "rm -r dist webpack-assets.json",
    "dev": "export NODE_ENV=development && yarn run build && yarn start",
    "build": "yarn run build:client && yarn run build:server",
    "start": "node dist/server/server.js",
    "build:client": "webpack --config=webpack.client.babel.js",
    "build:server": "webpack --config=webpack.server.babel.js",
    "analytics": "export NODE_ENV=production && webpack --config=webpack.analytics.babel.js",
    "snyk-protect": "snyk protect",
    "prepare": "yarn run snyk-protect"
  },
  "main": "index.js",
  "repository": "git@github.com:slashtu/pwa.git",
  "author": "Slash Tu <y12341234@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.15.5",
    "isomorphic-unfetch": "^2.0.0",
    "jade": "~1.11.0",
    "lodash": "^4.17.5",
    "morgan": "~1.9.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-loadable": "^5.3.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.3.0",
    "serve-favicon": "~2.4.5",
    "snyk": "^1.316.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "8.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-spread": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.18.2",
    "eslint-config-prettier": "^2.9.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "prettier": "^1.12.1",
    "webpack": "3.11.0",
    "webpack-bundle-analyzer": "^2.9.2",
    "webpack-manifest-plugin": "^2.0.0-rc.2",
    "webpack-merge": "^4.1.2",
    "webpack-node-externals": "^1.6.0"
  },
  "engines": {
    "node": "^8.9.0"
  },
  "snyk": true
}
