{
  "name": "react-pwa",
  "version": "1.0.0",
  "scripts": {
    "start": "webpack --config=webpack.config.babel.js && babel-node src/server/index.js",
    "build": "webpack -p --config ./webpack.config.js",
    "build:browser:dev": "webpack --env=dev --config=webpack.browser.dev.babel.js --progress --profile --colors --json > compilation-stats.json",
    "build:server:dev": "webpack --env=dev --config=webpack.server.dev.babel.js --progress --profile --colors"
  },
  "main": "index.js",
  "repository": "git@github.com:slashtu/pwa.git",
  "author": "Slash Tu <y12341234@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.15.5",
    "jade": "~1.11.0",
    "lodash": "^4.17.5",
    "morgan": "~1.9.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-loadable": "^5.3.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "serve-favicon": "~2.4.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "webpack": "3.11.0",
    "webpack-bundle-analyzer": "^2.9.2"
  }
}
